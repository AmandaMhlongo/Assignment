<!DOCTYPE HTML>
<html>
<head>
    <title>Flick's Hotel</title>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,700' rel='stylesheet' type='text/css'>

    @RenderSection("Bootstrapie", required: false)
    @RenderSection("Cont", required: false)
    @RenderSection("room", required: false)
    @RenderSection("Rsv", required: false)
    @RenderSection("Slide", required: false)


</head>
<body>
    <!-- start header -->
    <div class="header_bg">
        <div class="wrap">
            <div class="header">
                <div class="logo">


                    <a href="~/AddCustomers/HomePage"><img src="@Url.Content("~/Dumi/")images/logo.png" alt=""></a>
                </div>
                <div class="h_right">
                    <!--start menu -->
                    <ul class="menu">
                        <li class="active"><a href="~/AddCustomers/HomePage">Home</a></li> |
                        <li><a href="~/AddReservations/reservation">reservation</a></li> |
                        <li><a href="~/AddRooms/rooms">rooms & suits</a></li> |
                        <li><a href="~/AddContactUs/contact">contact Us</a></li>|
                        <li><a href="~/AddContactUs/About">About </a></li> |                            
                        <li>@Html.Partial("_LoginPartial")

                        </li>        
                       
                        <div class="clear"></div>
                    </ul>
                    <!-- start profile_details -->
                 
                </div>
                <div class="clear"></div>
                <div class="top-nav">
                    <nav class="clearfix">
                        <ul>
                            <li class="active"><a href="index.html">hotel</a></li>
                            <li><a href="rooms.html">rooms & suits</a></li>
                            <li><a href="reservation.html">reservation</a></li>
                            <li><a href="activities.html">activities</a></li>
                            <li><a href="contact.html">contact</a></li>
                        </ul>
                        <a href="#" id="pull">Menu</a>
                    </nav>
                </div>
            </div>
        </div>
    </div>



    @RenderBody()



    <div class="footer_bg">
        <div class="wrap">
            <div class="footer">
                <div class="copy">
                    <p class="link"><span>© All rights reserved |Developed by&nbsp;<a href="http://wam.com/"> The WAM</a></span></p>
                </div>
                <div class="f_nav">
                    <li><a href="index.html">home</a></li>
                    <ul>
                        <li><a href="rooms.html">rooms & suits</a></li>
                        <li><a href="reservation.html">reservation</a></li>
                        <li><a href="contact.html">Contact</a></li>
                    </ul>
                </div>
                <div class="soc_icons">
                    <ul>
                        <li><a class="icon1" href="#"></a></li>
                        <li><a class="icon2" href="#"></a></li>
                        <li><a class="icon3" href="#"></a></li>
                        <li><a class="icon4" href="#"></a></li>
                        <li><a class="icon5" href="#"></a></li>
                        <div class="clear"></div>
                    </ul>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </div>


    @RenderSection("scripts", required: false)

</body>
</html>






